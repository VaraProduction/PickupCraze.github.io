<?xml version="1.0" encoding="UTF-8"?>
<widget id="com.pickup.craze" version="1.0.0.0" android-versionCode="1000000" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
	<name>Pickup Craze</name>
	<description>Physics Game to collect coins and reach next levels.</description>
	<author email="akaappadoo@umail.utm.ac.mu" href="">VARA</author>
	<content src="index.html" />
	<allow-intent href="http://*/*" />
	<allow-intent href="https://*/*" />
	<allow-navigation href="https://*/*" />
	<allow-navigation href="blob:*" />
	<icon src="www/icons/icon-512.png" width="512" height="512" density="xxxhdpi" />
	<icon src="www/icons/icon-256.png" width="256" height="256" />
	<icon src="www/icons/icon-128.png" width="128" height="130" density="xhdpi" />
	<icon src="www/icons/icon-64.png" width="64" height="64" density="mdpi" />
	<icon src="www/icons/icon-32.png" width="32" height="32" />
	<icon src="www/icons/icon-16.png" width="16" height="16" />
	<plugin name="admob-plus-cordova" spec="1.28.0">
		<variable name="APP_ID_ANDROID" value="ca-app-pub-4473465325083725~4488621339" />
	</plugin>
	<plugin name="cordova-plugin-consent" spec="2.2.0" />
	<plugin name="cordova-plugin-device" spec="2.0.3" />
	<plugin name="construct-mobile-export" spec="1.0.0" />
	<preference name="orientation" value="landscape" />
	<preference name="fullscreen" value="false" />
	<preference name="DisallowOverscroll" value="true" />
	<preference name="LoadUrlTimeoutValue" value="120000" />
	<access origin="*" />
	<preference name="AutoHideSplashScreen" value="false" />
	<preference name="ShowSplashScreenSpinner" value="false" />
	<engine name="android" spec="11.0.0" />
	<platform name="android">
		<preference name="android-minSdkVersion" value="22" />
		<preference name="android-targetSdkVersion" value="32" />
		<preference name="AndroidWindowSplashScreenAnimatedIcon" value="www/icons/loading-logo.png" />
		<preference name="AndroidWindowSplashScreenBackground" value="#ffffff" />
	</platform>
</widget>
